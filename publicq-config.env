# publicq-config.env
# University settings
INSTITUTION_NAME="American University of Yangon"
INSTITUTION_LOGO="/logo.png"
ALLOW_REGISTRATION=false  # Students created by admin only

# Authentication
AUTH_TYPE="jwt"
JWT_SECRET="your-secret-key-here"

# Database
DB_TYPE="postgresql"
DB_CONNECTION="postgresql://localhost:5432/publicq"

# Features
ENABLE_AI_MONKEY=true
ENABLE_VERSIONING=true
MAX_UPLOAD_SIZE="10MB"
